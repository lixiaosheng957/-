page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;    
  margin: 0;
}
page{
  /* overflow: hidden; */
  /* height: 100%; */
  background-color: #F4F4F4;
}
.wrap{
  overflow: hidden;
}
.nav{
  width: 100%;
  /* height: 38rpx; */
  background: #FFFFFF;
  text-align: center;
  display: flex;
  position: fixed;
  z-index: 999;
}
.nav_item{
  width: 50%;
  height: 76rpx;
  line-height: 76rpx;
  position: relative;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0;
}
.nav_item.active{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #1FCEB3;
  letter-spacing: 0;
}
.nav>.nav_item:first-child::before{
  position: absolute;
  content: ' ';
  width: 1.2rpx;
  height: 32rpx;
  background-color: #D5D5D5;
  right: 0;
  top: 22rpx;
}
.nav_left{
  text-align: left;
}
.nav_left>text{
  margin-left: 88.6rpx;
}
.red_list{
  margin-top: 76rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 1130rpx;
  background-color: #F4F4F4; 
}
.red_item{
  width:  690rpx;
  height: 200rpx;
  background-size: 100%;
  background-image: url(data:image/png;base64,);
  margin-top: 24rpx;
  display: flex;
  position: relative;
}
.money_sign{
  width: 20rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #FF5340;
  letter-spacing: 0;
  text-align: left;
  position: absolute;
  left: 50rpx;
  top: 82rpx;
}
.money_num{
  width: 92rpx;
  height: 92rpx;
  line-height: 92rpx;
  font-family: PingFangSC-Regular;
  font-size: 72rpx;
  color: #FF5340;
  letter-spacing: 0;
  text-align: left;
  position: absolute;
  left: 82rpx;
  top: 40rpx;
}
.item_type{
  text-align: center;
  /* width: 42rpx; */
  height: 36rpx;
  padding: 0 8rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #1FCEB3;
  border: 1rpx solid #1FCEB3;
  border-radius: 5rpx;
  position: absolute;
  left: 244rpx;
  top: 46rpx;
}
.text_list{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #666666;
  letter-spacing: 0;
  text-align: left;
  position: absolute;
  height: 68rpx;
  top: 94rpx;
  left: 258rpx;
}
.text_item{
  position: relative;
}
.text_item::after{
  position: absolute;
  content:  ' ';
  display: block;
  width: 6rpx;
  height: 6rpx;
  overflow: hidden;
  left: -16rpx;
  top: 13rpx;
  background-color: #666666;
}
.goto_use{
  display: block;
  background: #FF5340;
  border-radius: 5rpx; 
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  width: 110rpx;
  height: 44rpx;
  line-height: 44rpx;
  box-sizing: border-box;
  position: absolute;
  top: 78rpx;
  right: 32rpx;

}
.red_description{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: left;
  width: 96rpx;
  height:34rpx;
  line-height: 34rpx;
  margin: 0 auto;
  margin-top:34rpx ;
}
.status_red{
  margin-top: 34rpx;
  font-size: 28rpx;
  color: #999999;
  text-align: center;
  display: flex;
}