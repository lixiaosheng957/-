page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;    
  margin: 0;
}
page{
  height: 100%;
  background-color: #F4F4F4;
  overflow: hidden;
}
.position_info_list{
  background: #FFFFFF;
  width: 100%;
  height: 80px;
  margin-top: 12px;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.position_img{
  width: 17px;
  height: 22px;
  position: absolute;
  top: 14px;
  left: 13px;
}
.position{
  position: absolute;
  top: 13px;
  left: 35px;
  height: 22px;
  font-family: PingFangSC-Medium;
  font-size: 16px;
  color: #999999;
  line-height: 22px;
}
.scan_img{
  width: 17px;
  height: 16px;
  position: absolute;
  right: 16px;
  bottom: 44px;
}
.edit_info{
  width: 72px;
  height: 22px;
  line-height: 22px;
  position: absolute;
  top: 16px;
  right: 12px;
  background-size: 100%;
  background-image: url(data:image/png;base64,);
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #1FCEB3;
  letter-spacing: 0;
  text-align: center;
}
.user_info{
 
  position: absolute;
  top: 43px;
  left: 35px;
}
.user_info>text{
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #999999;
  height: 20px;
  line-height: 20px;
}
.user_info>text:first-child{
    margin-right: 12px;
}
.install_info_list{
  margin-top: 12px;
}
.install_item{
  width: 100%;
  background-color: #ffffff;
  position: relative;
}
.boxborder{
  border-bottom: 1px solid #E5E5E5;
}
.pickerbox{
  width: 90%;
  height: 48px;
  margin: 0 auto;
  background-color: #ffffff;
}
.choose_exp{
  height: 20px;
  line-height: 20px;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  position: absolute;
  top: 14px;
  left:11px;
}
.choose_item{
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999999;
  letter-spacing: 0;
  text-align: left;
  line-height: 16px;
  height: 16px;
  line-height: 16px;
  position: absolute;
  right: 25px;
  bottom: 16px;
}
.arrow_img{
  width: 5.7px;
  height: 10px;
  position: absolute;
  left: 357px;
  bottom: 19px;
}
.protection{
  background: #FFFFFF;
  width: 100%;
  height: 64px;
  position: relative;
  margin-top: 14px;
}
.phone_protection{
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  height: 20px;
  line-height: 20px;
  position: absolute;
  top: 14px;
  left: 11px;
}
.protection_info{
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #999999;
  letter-spacing: 0;
  text-align: left;
  height: 16px;
  line-height: 16px;
  position: absolute;
  left: 11px;
  top: 37px;
}
.goto_pay{
  width: 100%;
  height: 48px;
  background: #464648;
  position: fixed;
  bottom: 0;
  line-height: 48px;
}
.pay_money{
  font-family: PingFangSC-Regular;
  font-size: 18px;
  color: #FFFFFF;
  letter-spacing: -0.43px;
  text-align: center;
  height: 25px;
  line-height: 25px;
  position: absolute;
  left: 12px;
  top: 12px;
}
.pay_btn{
  width: 105px;
  height: 48px;
  position: absolute;
  right: 0;
}
.pay_btn>view{
  width: 105px;
  height: 48px;
  font-family: PingFangSC-Regular;
  font-size: 18px;
  color: #FFFFFF;
  background: #FB0000;
  letter-spacing: -0.78px;
  line-height: 48px;
  text-align: center;
}