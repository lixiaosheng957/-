.frame {
  width: 750rpx;
  margin-top: 26rpx;
  box-sizing: border-box;
  background: #FFFFFF;
  border: 1rpx solid #E4E4E4;
}

.question {
  width: 681.8rpx;
  /* font-family: PingFangSC-Regular; */
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  line-height: 42rpx;
  margin-left: 28.2rpx;
  margin-top: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.line {
  width: 703.2rpx;
  border: 1rpx solid #E4E4E4;
  margin-top: 21.6rpx;
  margin-left: 23.4rpx;
}

.answer {
  width: 703.2rpx;
  /* font-family: PingFangSC-Regular; */
  margin-top: 10rpx;
  margin-left: 28.2rpx;
  margin-bottom: 20.6rpx;
  font-size: 26rpx;
  color: #666666;
  letter-spacing: 0;
  text-align: left;
  line-height: 46rpx;
}